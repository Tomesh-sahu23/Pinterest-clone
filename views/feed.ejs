<%- include('partials/header'); %>
<div class="w-full min-h-screen bg-zinc-800 px-10 py-8 text-white">
    <div class="cards flex  flex-wrap gap-5 ">
        <% posts.forEach(function(elem){ %>
            <div class=" card w-32">
                <div class=" w-full h-56 bg-zinc-100 rounded-lg overflow-hidden">
                    <img class="w-full h-full object-cover" src="/images/uploads/<%= elem.image %>" alt="">
                </div>
                <h5 class="mt-2 text-xs"><%= elem.title %></h5>
                <a href="#" class="text-xs opacity-500">@<%= elem.user.username %></a href="#">
            </div>
        <% }) %>
    </div>
</div>
<%- include('partials/footer'); %>